<!DOCTYPE html>
<html lang="en" ng-app ="myApp">
<head>
    <title>Challenge&Play</title>
    <% include templates/head %>
    <!-- Material Design Bootstrap -->
    <link href="css/table.css" rel="stylesheet">

    <style type="text/css">
        img { width: 140px; height: 140px; border-radius: 50%!important }
    </style>

</head>

<body>

    <header>
      <% include templates/header %>
    </header>

    <main>
        <div class="col-sm-12 en-medio">
          <div class="text-center">
            <div class="row">
                <div class="col-sm-4 offset-4 align-middle ">
                    <div class="">
                        <form class="form-nueva-clase">
                          <h3>Nueva clase</h3>
                          <div class="form-group">
                            <input type="email" class="form-control" id="nombre-clase" aria-describedby="emailHelp" placeholder="Nombre de la clase">
                          </div>
                          <div class="form-group">
                            <input type="email" class="form-control" id="curso-clase" aria-describedby="emailHelp" placeholder="Curso">
                          </div>
                          <div class="form-group">
                            <input type="password" class="form-control" id="pass-clase" placeholder="Password para acceder a la clase">
                          </div>
                          <div class="form-group">
                            <input type="password" class="form-control" id="pass-clase2" placeholder="Repita password para acceder a la clase">
                          </div>
                          <button type="submit" class="btn red">ENVIAR</button>
                        </form>
                    </div>
                </div>
            </div>
          </div>
        </div>
    </main>

<!--Footer-->

<footer class="page-footer center-on-small-only">
  <% include templates/footer %>
</footer>

<!--/.Footer-->

  <!-- SCRIPTS -->
  <% include templates/scripts %>
  <!-- /.SCRIPTS -->

</body>
</html>
